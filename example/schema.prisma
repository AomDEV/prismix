model Client {
  id         String  @id
  ip_address String?
  account_id String
  account    Account @relation(name: "AccountToClient", fields: [account_id], references: [id])


  @@map("clients")
}

model Account {
  id      String   @id
  clients Client[] @relation(name: "AccountToClient")


  @@map("accounts")
}
